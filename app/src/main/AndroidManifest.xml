<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.avershihina.astronomy_handbook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/my_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/my_icon_round"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Holo" >
        <activity android:name="ru.avershihina.astronomy_handbook.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ru.avershihina.astronomy_handbook.MenuActivity"
            android:label="Главное меню"
            android:theme="@android:style/Theme.NoTitleBar" />
        <activity
            android:name="ru.avershihina.astronomy_handbook.PlanetsActivity"
            android:label="@string/planets"
            android:theme="@android:style/Theme.NoTitleBar" />
        <activity
            android:name="ru.avershihina.astronomy_handbook.SolarSystemActivity"
            android:label="@string/solar_system"
            android:theme="@android:style/Theme.NoTitleBar"  />
        <activity
            android:name="ru.avershihina.astronomy_handbook.StarsActivity"
            android:label="@string/stars"
            android:theme="@android:style/Theme.NoTitleBar"  />
        <activity
            android:name="ru.avershihina.astronomy_handbook.MilkyWayActivity"
            android:label="@string/milky_way"
            android:theme="@android:style/Theme.NoTitleBar"  />
        <activity
            android:name="ru.avershihina.astronomy_handbook.UniverseActivity"
            android:label="@string/universe"
            android:theme="@android:style/Theme.NoTitleBar" />
        <activity
            android:name="ru.avershihina.astronomy_handbook.SpaceTimeActivity"
            android:label="@string/space_time"
            android:theme="@android:style/Theme.NoTitleBar"  />
        <activity
            android:name="ru.avershihina.astronomy_handbook.ArticleActivity"
            android:label="@string/article"
            android:theme="@android:style/Theme.NoTitleBar"  />
    </application>

</manifest>